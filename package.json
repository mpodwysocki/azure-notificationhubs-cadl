{
  "name": "azure-notificationhubs-cadl",
  "version": "1.0.0",
  "description": "Azure Notification Hubs CADL Generator",
  "main": "index.js",
  "cadlMain": "main.cadl",
  "scripts": {
    "build": "npm run build:openapi3 && npm run build:python",
    "build:csharp": "cadl compile main.cadl --emit ../autorest.csharp/src/CADL.Extension/Emitter.Csharp/dist/src/index.js --output-path cadl-output/csharp",
    "build:java": "cadl compile main.cadl --emit ../autorest.java/cadl-extension/dist/src/index.js --output-path cadl-output/java",
    "build:openapi3": "cadl compile main.cadl --emit @cadl-lang/openapi3 --output-path cadl-output/openapi3",
    "build:python": "cadl compile main.cadl --emit @azure-tools/cadl-python --output-path cadl-output/python"
  },
  "keywords": [],
  "author": "Matthew Podwysocki",
  "license": "MIT",
  "dependencies": {
    "@cadl-lang/compiler": "^0.35.0",
    "@cadl-lang/openapi3": "^0.15.0",
    "@cadl-lang/rest": "^0.17.0",
    "@azure-tools/cadl-csharp": "^0.1.4",
    "@azure-tools/cadl-python": "^0.2.1"
  }
}
